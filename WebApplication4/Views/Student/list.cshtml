
@{
    ViewBag.Title = "list";
}


<h2>List</h2>
<table class="table table-bordered">
    <tr>
        <th>Name</th>
        <th>Id</th>
        <th>Address</th>
        <th>Email</th>
    </tr>
    @foreach (var item in Model)
    {
<tr>
    <td>@item.Name</td>
    <td>@item.Id</td>
    <td>@item.Address</td>
</tr>}
</table>
